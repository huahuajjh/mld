@{
    Layout = null;
}
@model IList<MLD.ViewModel.Back.User>
<div class="modal-header">
    <button type="button" class="close" data-dismiss="modal" aria-hidden="true">×</button>
    <h4 class="modal-title">课程预约</h4>
</div>
<div class="modal-body">
    <table class="table table-striped">
        <thead>
        <tr>
            <th>编号</th>
            <th>预约姓名</th>
            <th>预约联系号码</th>
            <th>预约状态</th>
        </tr>
        </thead>
        <tbody>
       @for (int i = 0; i < Model.Count; i++)
       {
           var item = Model[i];
           <tr>
               <td>@i</td>
               <td>@item.Name</td>
               <td>@item.Tel</td>
               <td>@item.ReservationState.GetEnumName()</td>
           </tr>
       }
        </tbody>
    </table>
</div>
